  \lstick{\ket{x_2}} & \gate{Z} & \ctrl{1} & \qw      & \qw      & \qw      & \qw      & \qw      & \qw \\
  \lstick{\ket{x_3}} & \qw      & \gate{Z} & \ctrl{1} & \qw      & \qw      & \qw      & \qw      & \qw \\
  \lstick{\ket{x_4}} & \qw      & \qw      & \gate{Z} & \ctrl{1} & \_
